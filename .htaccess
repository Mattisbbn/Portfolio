RewriteEngine On
RewriteBase /
# Redirige toutes les requÃªtes sauf si le fichier ou le dossier existe
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php [QSA,L]
<Files config.php>
    Order Allow,Deny
    Deny from all
</Files>

<Files package.json>
    Order Allow,Deny
    Deny from all
</Files>